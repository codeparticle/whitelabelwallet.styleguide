{
    "presets": [
        ["@babel/preset-react"],
        "@babel/env"
    ],
    "plugins": [
        "@babel/plugin-proposal-export-namespace-from",
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
                "src": "./src",
                "components": "./src/components",
                "styles": "./src/styles",
                "svgs": "./src/svgs"
            }
        }]
    ]
}
